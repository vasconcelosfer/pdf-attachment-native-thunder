{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.2",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "browser_specific_settings": {
    "gecko": {
      "id": "native-pdf-final@arca.local",
      "strict_min_version": "115.0"
    }
  },
  "icons": {
    "16": "icons/icon.svg",
    "32": "icons/icon.svg",
    "48": "icons/icon.svg",
    "64": "icons/icon.svg",
    "128": "icons/icon.svg"
  },
  "permissions": [
    "messagesRead",
    "downloads",
    "accountsRead",
    "activeTab",
    "notifications"
  ],
  "background": {
    "scripts": ["worker/pdf-lib.min.js", "background.js"]
  },
  "message_display_action": {
    "default_title": "__MSG_buttonTitle__",
    "default_icon": "icons/icon.png"
  },
  "experiment_apis": {
    "NativePdf": {
      "schema": "api/NativePdf/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "paths": [["NativePdf"]],
        "script": "api/NativePdf/implementation.js",
        "events": ["startup"]
      }
    }
  }
}